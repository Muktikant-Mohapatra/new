package specification;

import product.SizeSpecification;

public class AndSpecification<T> implements Specification<T> {
	private Specification<T> first, second;

	public AndSpecification(SizeSpecification sizeSpecification, ColourSpecification colourSpecification) {
		// TODO Auto-generated constructor stub
	}

	@Override
	public boolean isSatisfied(T items) {

		return first.isSatisfied(items) && second.isSatisfied(items);
	}
}
