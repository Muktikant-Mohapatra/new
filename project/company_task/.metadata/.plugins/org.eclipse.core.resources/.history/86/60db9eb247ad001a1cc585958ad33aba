package com.parking.main;

import com.parking.domain.model.Car;
import com.parking.domain.model.ParkingLot;
import com.parking.service.CarParkingService;
import com.parking.service.ParkingService;

public class Parking {

	public static void main(String[] args) {
		ParkingLot parkingArea = new ParkingLot(1000, "middle");
		ParkingService service = new CarParkingService();
		Car car1 = new Car("1234", "white");
		Car car2 = new Car("4568", "red");
		Car car3 = new Car("0000", "blue");
		Car car4 = new Car("1111", "green");
		Car car5 = new Car("5555", "violet");
		    service.park(car1, parkingArea);
			service.park(car2, parkingArea);
			service.park(car3, parkingArea);
			service.park(car4, parkingArea);
			service.park(car4, parkingArea);
			service.park(car4, parkingArea);
			service.park(car4, parkingArea);
			service.park(car4, parkingArea);
			service.park(car4, parkingArea);
			service.emptySlot();

	}
}
