package com.parking.dto;

import java.util.Arrays;
import java.util.Date;

public class ParkingReceiptDto {
	private String id;
	private CarDto[] car;

	public ParkingReceiptDto(String id, CarDto[] car) {
		super();
		this.id = id;
		this.car = car;
	}

	@Override
	public String toString() {
		return "ParkingReceiptDto [id=" + id + ", car=" + Arrays.toString(car) + "]";
	}

}
